<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/>

    <title>{% block title %}{{ 'admin.title'|trans }} - {{ 'site.name'|trans }}{% endblock %}</title>

    <meta name="keywords" content="{% block page_keywords %}{% endblock %}">
    <meta name="description" content="{% block page_description %}{% endblock %}">

    <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('build/images/favicon/favicon-32x32.png') }}">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('build/images/favicon/favicon-16x16.png') }}">
    <link rel="shortcut icon" href="{{ asset('build/images/favicon/favicon.ico') }}">

    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="msapplication-config" content="{{ asset('build/images/favicon/browserconfig.xml') }}">
    <meta name="theme-color" content="#ffffff">

    {{ encore_entry_link_tags('section-admin') }}
    {% block stylesheets %}{% endblock %}
</head>

{% block layout_body %}{% endblock %}

{{ encore_entry_script_tags('section-admin') }}
</html>
